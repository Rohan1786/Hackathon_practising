<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Profile</title>
</head>
<body>
  <h1>Profile</h1>
  <h2>Order Records</h2>
  <table border="1">
    <tr>
      <th>Username</th>
      <th>Order Count</th>
    </tr>
    <% orderRecords.forEach(record => { %>
      <tr>
        <td><%= record.username %></td>
        <td><%= record.count %></td>
      </tr>
    <% }); %>
  </table>
</body>
</html>
